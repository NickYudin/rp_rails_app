<div class="grid-container">

	<div>
		<% if current_user.admin? && image.non_accepted %>
		
		<% end %>
	</div>
	<% @images.each do |image| %>
		<div class="card shadow" style="width: 18rem;">
			<% if image.accepted %>
				<%= image_tag image.url, class: "img-fluid shadow" if image.url? %>
					<div class="card-body text-center">
				    	<%= link_to "Go to image", image,  class: "btn btn-warning shadow"%>
					</div>
			<% end %>
		</div>
	<% end %>
</div>